    <title>mesclandoArraysRemovendoDuplicatas</title>
</head>
<body>
    <script>
        function mesclarEremoverDuplicatas (array1,array2){
            let mesclado = array1.concat(array2);
            return [...new Set(mesclado)];
        }

        let array1 = [1,2,3];
        let array2 = [1,3,4,5];
        let resultado = mesclarEremoverDuplicatas(array1, array2);
        console.log(resultado);
    </script>
</body>
</html>
