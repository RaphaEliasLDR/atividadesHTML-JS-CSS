    <h1>Testando js</h1>
    <script>



let numeros = [];
for (let i = 1; i <=100; i++){
    numeros.push(i);
}

let pares = numeros.filter(function(numero){
    return numero % 2 === 0;
});



console.log(pares);
    </script>
