<body>
    <script>
        function maiorNumero(array){
            return array.reduce(function(max, atual){
            return atual > max ? atual : max;
        });
    }

    let numeros = [10,30,40,50,60,70];
    let maior = maiorNumero(numeros);
    console.log(maior);
        </script>
</body>
</html>
