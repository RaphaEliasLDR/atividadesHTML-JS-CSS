<body>
    <script>
        let numeros = [1,2,3,4,5];
        let dobrados = numeros.map (function(numero){
        return numero * 2;
    });

        let soma = numeros.map (function(numero){
            return numero + 2;
        });

        let divisao = numeros.map (function(numero){
            return numero / 2;
        })
        let subtracao = numeros.map (function(numero){
            return numero - 2;
        })

    console.log(dobrados);
    console.log(soma);
    console.log(divisao);
    console.log(subtracao);
    </script>
    
</body>
</html>
